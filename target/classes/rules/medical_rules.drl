// Indeks Massa Tubuh

rule "Rule E1 - BMI for Subject 1 - Kekurangan berat badan tingkat berat"
    when
        $patient : Patient(
            gender == "female" || gender == "male", 
            age <= 59, 
            pregnant == false, 
            alcoholConsumption == false,
            smoking == false, 
            weight != null && weight > 0, 
            height != null && height > 0,
            $bmi : weight / (Math.pow(height / 100, 2)) < 17
        )
