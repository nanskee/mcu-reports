package rules

import com.medicalrecommendation.model.Patient;

// Blood Pressure

rule "Rule A1 - Blood Pressure for Subject 1 - Normal"
    when
        $patient : Patient(
            gender == "female" || gender == "male", 
            age <= 59, 
            pregnant == false, 
            alcoholConsumption == false,
            smoking == false, 
            systolicPressure < 130 && diastolicPressure < 85
        )
    then
        $patient.setBloodPressureRecommendation(
            "You are advised to exercise regularly, such as walking, jogging, or swimming." +
            "Limit sodium intake to 2,300 mg per day and consume a well-balanced, nutritious diet rich in " +
            "grains, fruits, vegetables, low-fat dairy products, and adequate amounts of potassium. " +
            "Minimize stress and aim for 7–9 hours of sleep daily to maintain optimal health."
        );
end

rule "Rule A2 - Blood Pressure for Subject 1 - Pra Hipertensi"
    when
        $patient : Patient(
            gender == "female" || gender == "male",
            age <= 59,
            pregnant == false,
            alcoholConsumption == false,
            smoking == false,
            systolicPressure >= 130 && systolicPressure <= 139 ||
            diastolicPressure >= 85 && diastolicPressure <= 89
        )
    then
        $patient.setBloodPressureRecommendation(
            "Anda disarankan membatasi natrium hingga 1.500 mg per hari(?), serta mengurangi garam, makanan berpengawet, dan kafein jika sensitif. Konsumsi makanan bergizi seimbang yang kaya serat, biji-bijian, buah, sayur, dan produk susu rendah lemak. Rutin berolahraga, seperti berjalan kaki atau joging. Kelola stres dan tidur 7-9 jam per hari untuk kesehatan optimal.\n\n" +
            "---\n" +
            "Anda mendapat rekomendasi ini karena asupan natrium yang berlebihan dapat " +
            "meningkatkan tekanan darah dengan menarik lebih banyak cairan ke dalam pembuluh darah, " +
            "sehingga meningkatkan beban kerja jantung. Membatasi natrium membantu menjaga tekanan " +
            "darah tetap stabil dan mengurangi risiko berkembangnya hipertensi."
        );
end

rule "Rule A3 - Blood Pressure for Subject 1 - Hipertensi tingkat 1"
    when
        $patient : Patient(
            gender == "female" || gender == "male", 
            age <= 59, 
            pregnant == false, 
            alcoholConsumption == false,
            smoking == false, 
            systolicPressure >= 140 && systolicPressure <= 159 || 
            diastolicPressure >= 90 && diastolicPressure <= 99
        )
    then
        $patient.setBloodPressureRecommendation(
            "Anda disarankan membatasi natrium hingga 1.500 mg per hari serta mengurangi garam, makanan berpengawet, dan kafein jika sensitif. Konsumsi makanan bergizi kaya serat, biji-bijian, buah, sayur, dan produk susu rendah lemak. Lakukan olahraga aerobik sedang* seperti jalan cepat atau berenang. Kelola stres, tidur cukup, dan pantau tekanan darah rutin. Jika gaya hidup tak cukup atau berisiko, dokter bisa meresepkan obat. Jika tekanan darah meningkat menjadi >= 160, segera cari bantuan medis.\n\n" +
            "---\n" +
            "Anda mendapat rekomendasi ini karena olahraga aerobik sedang, seperti jalan cepat atau berenang, " +
            "membantu melancarkan sirkulasi darah, menurunkan tekanan darah, dan menjaga kesehatan jantung " +
            "tanpa membebani tubuh secara berlebihan."
        );
end

